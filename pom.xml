                <groupId>com.wx.boot</groupId>
                <artifactId>wx-common-rpc-dubbo</artifactId>
                <version>${project.version}</version>
            </dependency>
            <!--            -->
            <dependency>
                <groupId>com.wx.boot</groupId>
                <artifactId>wx-module-usercenter-api</artifactId>
                <version>${project.version}</version>
            </dependency>
