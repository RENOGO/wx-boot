            <dependency>
                <groupId>com.wx.boot</groupId>
                <artifactId>wx-common-swagger</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.wx.boot</groupId>
                <artifactId>wx-common-lock-redis</artifactId>
